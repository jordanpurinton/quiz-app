<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site.master.cs" Inherits="QuizMania.SiteMaster" %>

<!DOCTYPE html>

<html lang="en">
<head runat="server">
    <title>Quiz Mania</title>
    <link rel="stylesheet" href="Content/stylesheet.css" type="text/css" />
    <style type="text/css">
